{
  "name": "ql-riot",
  "version": "0.0.1",
  "description": "RiotJS components",
  "repository": "https://github.com/qlbit-com/ql-riot",
  "main": "root/main.js",
  "author": "Dmitriy Pichugin",
  "license": "MIT",
  "engines": {
    "node": ">=4.0.0"
  },
  "scripts": {
    "cov": "nyc report --reporter=text-lcov",
    "cov-html": "nyc report --reporter=html",
    "build": "rollup -c",
    "release": "rollup -c --environment mode:production",
    "prepublishOnly": "npm test",
    "start": "npm run watch & npm run server",
    "watch": "rollup -c -w 'src/**'",
    "server": "browser-sync start --files './root/*, style.css, index.html' --server",
    "test": "nyc --require esm --require jsdom-global/register --require @riotjs/register mocha src/**/*.spec.js"
  },
  "keywords": [
    "riot",
    "rollup"
  ],
  "devDependencies": {
    "@riotjs/compiler": "^6.4.2",
    "@riotjs/register": "^6.0.1",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.4.0",
    "browser-sync": "^2.28.3",
    "rollup": "^3.19.1",
    "rollup-plugin-riot": "^6.0.0",
    "fast-glob": "^3.2.12"
  },
  "dependencies": {
    "riot": "^7.1.0",
    "riot-pure-props": "^1.0.2",
    "axios": "^1.3.4"
  }
}
